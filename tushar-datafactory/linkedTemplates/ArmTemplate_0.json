{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tushar-datafactory"
		},
		"tushar02_cont02_ActivityLog_CSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'tushar02_cont02_ActivityLog_CSV'"
		},
		"tusharworkspace_tusharpool_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'tusharworkspace_tusharpool'"
		},
		"tushar02_cont02_ActivityLog_CSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tushar02.dfs.core.windows.net/"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tusharsynapseworkspace.sql.azuresynapse.net"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "tusharpool1"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/tushar02_cont02_ActivityLog_CSV')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('tushar02_cont02_ActivityLog_CSV_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('tushar02_cont02_ActivityLog_CSV_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/tusharworkspace_tusharpool')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"server": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_server')]",
					"database": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('tusharworkspace_tusharpool_password')]"
					}
				}
			},
			"dependsOn": []
		},
