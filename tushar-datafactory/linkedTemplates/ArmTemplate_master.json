{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tushar-datafactory"
		},
		"tushar02_cont02_ActivityLog_CSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'tushar02_cont02_ActivityLog_CSV'"
		},
		"tusharworkspace_tusharpool_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'tusharworkspace_tusharpool'"
		},
		"tushar02_cont02_ActivityLog_CSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tushar02.dfs.core.windows.net/"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tusharsynapseworkspace.sql.azuresynapse.net"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "tusharpool1"
		},
		"tusharworkspace_tusharpool_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tushar-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tushar02_cont02_ActivityLog_CSV_accountKey": {
						"value": "[parameters('tushar02_cont02_ActivityLog_CSV_accountKey')]"
					},
					"tusharworkspace_tusharpool_password": {
						"value": "[parameters('tusharworkspace_tusharpool_password')]"
					},
					"tushar02_cont02_ActivityLog_CSV_properties_typeProperties_url": {
						"value": "[parameters('tushar02_cont02_ActivityLog_CSV_properties_typeProperties_url')]"
					},
					"tusharworkspace_tusharpool_properties_typeProperties_server": {
						"value": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_server')]"
					},
					"tusharworkspace_tusharpool_properties_typeProperties_database": {
						"value": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_database')]"
					},
					"tusharworkspace_tusharpool_properties_typeProperties_userName": {
						"value": "[parameters('tusharworkspace_tusharpool_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}